{
    "version": 2,
    "builds": [
      {
        "src": "api/login.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/data.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/login",
        "dest": "/api/login.js"
      },
      {
        "src": "/api/data",
        "dest": "/api/data.js"
      }
    ]
  }
  